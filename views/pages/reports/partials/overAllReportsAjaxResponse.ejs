<div class="card">
	<div class="card-body">
		<div class="row bg-dark text-white">
	    	<div class="form-group col-sm-3 border-bottom border-dark">
	    		<label for="year" class="form-control-label">Income Sources</label>
            </div>
            <div class="form-group col-sm-3 border-bottom border-dark">
	    		<label for="amount" class="form-control-label"><span>&#8377;</span>Income</label>
            </div>
            <div class="form-group col-sm-3 border-bottom border-dark">
	    		<label for="comment" class="form-control-label"><span>&#8377;</span>Expense</label>
            </div>
            <div class="form-group col-sm-3 border-bottom border-dark">
	    		<label for="comment" class="form-control-label"><span>&#8377;</span>Balance</label>
            </div>
	    </div>
		<% for(i=0;i<reportsData.data.length; i++) { %>
		    <div class="row">
		    	<div class="form-group col-sm-3 border-bottom border-dark">
		           <%= reportsData.data[i].income_source%>
		        </div>
		        <div class="form-group col-sm-3 border-bottom border-dark">
		           <span>&#8377;</span><%= reportsData.data[i].overall_income%>
		        </div>
		        <div class="form-group col-sm-3 border-bottom border-dark">
		           <span>&#8377;</span><%= reportsData.data[i].overall_expense%>
		        </div>
		        <div class="form-group col-sm-3 border-bottom border-dark">
		           <span>&#8377;</span><%= reportsData.data[i].overall_balance%>
		        </div>
		    </div>
	    <% } %>
	    <div class="row">
		    <div class="form-group col-sm-3 border-bottom border-dark"></div>
	    	<div class="form-group col-sm-3 border-bottom border-dark">
		        <span>&#8377;</span><%= reportsData.totalInc%>
		    </div>
		    <div class="form-group col-sm-3 border-bottom border-dark">
		        <span>&#8377;</span><%= reportsData.totalExp%>
		    </div>
		    <div class="form-group col-sm-3 border-bottom border-dark">
		    	<span>&#8377;</span><%= reportsData.balance%>
		    </div>
	    </div>
	</div>
</div>